                <ui:Instance template="keyValueEntry" name="sizeEntry">
                    <AttributeOverrides element-name="lblKey" text="Size:" />
                    <AttributeOverrides element-name="lbValue" text="N cm high, N cm wide" />
                </ui:Instance>
                <ui:Instance template="keyValueEntry" name="dietEntry">
                    <AttributeOverrides element-name="lblKey" text="Diet:" />
