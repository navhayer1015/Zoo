<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="False">
    <ui:Template name="keyValueEntry" src="project://database/Assets/UI/keyValueEntry.uxml?fileID=9197481963319205126&amp;guid=7e7ba2a0b6b484aab873856c085e9a3c&amp;type=3#keyValueEntry" />
    <ui:VisualElement name="root" style="flex-grow: 1; flex-direction: column-reverse; flex-wrap: wrap-reverse;">
        <ui:VisualElement name="navigationPanel" style="flex-direction: row;">
            <ui:Button text="Previous" display-tooltip-when-elided="true" />
            <ui:Button text="Next" display-tooltip-when-elided="true" />
